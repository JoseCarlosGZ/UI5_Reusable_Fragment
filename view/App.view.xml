<mvc:View controllerName="logaligroup.SAPUI5.controller.App"
          xmlns="sap.m" 
          xmlns:mvc="sap.ui.core.mvc"
          displayBlock="true">
    <Shell>
        <App id="app">
            <pages>
                <Page title="{i18n>titleAppPage}">
                    <headerContent>
                        <Button 
                                icon="sap-icon://action" 
                                press=".onOpenDialogHeader" 
                                class="sapUiSmallMarginEnd"/>
                    </headerContent>
                    <content>
                        <Button  id="id_btnNav"  text="navTo Secondary.view.xml" press=".onNavToSecondary" class="sapUiSmallMargin"/>
                            
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>

